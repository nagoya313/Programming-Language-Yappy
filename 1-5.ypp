import <yappyio>;

yappy_t main(vector<string> argc) {
  yappy::console::write("整数を入力してください：　");
  int num := yappy::parse<int>(yappy::console::readLine());
  yappy::console::write("浮動小数を入力してください：　");
  float f := yappy::parse<float>(yappy::console::readLine());
  yappy::console::write("{0} {1}", num, f);
  return 0;
}
